#include <bits/stdc++.h> 
#include <iostream>
#include <cstring>
using namespace std;

void printSubstrings(string &input) {
    for(int i=0;i<input.size();i++){
        string str = "";
        for(int j=i;j<input.size();j++){

            str+=input[j];
            cout << str << endl;
        }
        
    }
}

int main() {
    string input;
    getline(cin, input);
    printSubstrings(input);
    return 0;
}
